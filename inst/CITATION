citHeader("To cite package 'SGPdata' in publications use:")

citEntry(entry = "Manual",
         title        = "{SGPdata}: Exemplar data sets for SGP analyses.",
         author       = "Damian W. Betebenner and Adam Van Iwaarden and Ben Domingue",
         year         = "2012",
         note         = "R package version 1.0-0.0",
         url          = "http://cran.r-project.org/web/packages/SGPdata/index.html",
         
         textVersion  = paste("Damian W. Betebenner, Adam Van Iwaarden and Ben Domingue (2012).",
                        "SGPdata: Exemplar data sets for SGP analyses.",
                        "(R package version 1.0-0.0.",
                        "URL http://cran.r-project.org/web/packages/SGPdata/index.html")
)
