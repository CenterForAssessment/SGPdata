citHeader("To cite package 'SGPdata' in publications use:")

citEntry(entry = "Manual",
         title        = "{SGPdata}: Exemplar Data Sets for SGP Analyses.",
         author       = "Damian W. Betebenner and Adam R. Van Iwaarden and Ben Domingue",
         year         = "2018",
         note         = "R package version 20.0-0.0",
         url          = "https://centerforassessment.github.io/SGPdata/",
         
         textVersion  = paste("Damian W. Betebenner, Adam R. Van Iwaarden and Ben Domingue (2018).",
                        "SGPdata: Exemplar Data Sets for SGP Analyses.",
                        "(R package version 20.0-0.0.",
                        "URL https://centerforassessment.github.io/SGPdata/")
)
